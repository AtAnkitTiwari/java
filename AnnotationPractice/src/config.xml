<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
  <context:annotation-config/>
  
  <bean class="com.annotation.practice.Marks" name="mark" >
     <property name="math" value="95"/>
      <property name="physics" value="90"/>
       <property name="chemistery" value="85"/>
       </bean>
       <bean class="com.annotation.practice.Student" name="student">
       <property name="name" value="Aman Yadav"/>
       
     </bean>
  
  
   </beans>